<Window x:Class="EcoDatUnpacker.VersionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="えこだっとあんぱっかーのバージョン情報" ResizeMode="NoResize"
        WindowStartupLocation="CenterOwner" Initialized="Window_Initialized"
        ShowInTaskbar="False" mc:Ignorable="d"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        SizeToContent="WidthAndHeight"
        Icon="Images/bookmark_yellow.ico">
    <StackPanel Margin="9" MinWidth="300">
        <Image Margin="3" Source="Images/bookmark_yellow.ico" Width="48" Height="48" HorizontalAlignment="Left" />
        <StackPanel Margin="3">
            <TextBlock Text="えこだっとあんぱっかー" />
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="Version: " />
                <TextBlock HorizontalAlignment="Left" Name="versionTextBlock" Text="TextBlock" VerticalAlignment="Top" />
            </StackPanel>
            <TextBlock Text="(C) 2011 ShTair,Sheltie" />
        </StackPanel>
        <StackPanel Margin="3">
            <TextBlock Text="Icon: http://www.webdesignerdepot.com" />
        </StackPanel>
        <Button Content="OK" Margin="3" Name="button1" Height="23" HorizontalAlignment="Right" Width="75" Click="button1_Click" />
    </StackPanel>
</Window>
