<Window x:Class="EcoDatUnpacker.SspWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SSP-&gt;CSV変換" ResizeMode="NoResize" WindowStartupLocation="CenterOwner"
        ShowInTaskbar="False" mc:Ignorable="d"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        SizeToContent="WidthAndHeight">
    <StackPanel Margin="9">
        <StackPanel Margin="3">
            <TextBlock HorizontalAlignment="Left" Name="textBlock1" Text="この機能にはバグがある可能性があります。" />
            <TextBlock HorizontalAlignment="Left" Name="textBlock2" Text="通常は使用しないでください。" />
        </StackPanel>
        <CheckBox Margin="3" Content="全データの抽出を試みる" HorizontalAlignment="Left" Name="checkBox1" />
        <Button Margin="3" Content="実行" Height="23" Width="75" HorizontalAlignment="Center" Click="Button_Click" />
    </StackPanel>
</Window>
